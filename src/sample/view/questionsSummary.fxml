<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.QuestionsSummary">
   <children>
      <Label fx:id="question" layoutX="93.0" layoutY="82.0" prefHeight="17.0" prefWidth="368.0" text="Question" />
      <Label fx:id="userAnswer" layoutX="107.0" layoutY="138.0" prefHeight="17.0" prefWidth="368.0" text="User answer" />
      <Label fx:id="correctAnswer" layoutX="109.0" layoutY="211.0" prefHeight="17.0" prefWidth="403.0" text="Correct answer" />
      <Button fx:id="next" layoutX="330.0" layoutY="299.0" mnemonicParsing="false" onAction="#goToNextQuestion" text="Next" />
      <Button fx:id="menu" layoutX="191.0" layoutY="299.0" mnemonicParsing="false" onAction="#goToMenu" text="Menu" />
   </children>
</AnchorPane>
