<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.InputQuestion">
   <children>
      <Label fx:id="question" layoutX="157.0" layoutY="88.0" prefHeight="17.0" prefWidth="264.0" text="Question" />
      <TextField fx:id="input" layoutX="157.0" layoutY="159.0" promptText="Input" />
      <Button fx:id="answer" layoutX="157.0" layoutY="308.0" mnemonicParsing="false" onAction="#answerQuestion" text="Answer" />
      <Button fx:id="next" layoutX="339.0" layoutY="308.0" mnemonicParsing="false" onAction="#nextQuestion" text="Next" />
      <Label fx:id="alert" layoutX="157.0" layoutY="120.0" text="Alert" />
   </children>
</AnchorPane>
